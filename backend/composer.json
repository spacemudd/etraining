{
    "name": "spacemudd/etraining",
    "author": "Shafiq al-Shaar",
    "type": "project",
    "license": "proprietary",
    "description": "An eTraining platform",
    "keywords": [
        "framework",
        "laravel",
        "etraining"
    ],
    "require": {
        "php": "^7.3",
        "barryvdh/laravel-snappy": "^0.4.8",
        "brick/money": "^0.5.0",
        "brick/phonenumber": "^0.4.0",
        "cca-bheath/laravel-sms-clicksend": "^4.0",
        "components/font-awesome": "^5.15",
        "doctrine/dbal": "2.*",
        "fideloper/proxy": "^4.2",
        "firebase/php-jwt": "^5.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "h4cc/wkhtmltopdf-amd64": "^0.12.4",
        "inertiajs/inertia-laravel": "^0.4.0",
        "jamesmills/laravel-timezone": "^1.9",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/cashier-paddle": "^1.2",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.3",
        "laravel/horizon": "^5.3",
        "laravel/jetstream": "^2.0",
        "laravel/sanctum": "^2.6",
        "laravel/scout": "^8.5",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1",
        "macsidigital/laravel-zoom": "^4.1",
        "owen-it/laravel-auditing": "^11.0",
        "predis/predis": "^1.1",
        "protonemedia/inertiajs-tables-laravel-query-builder": "^1.4",
        "sentry/sentry-laravel": "^2.3",
        "skagarwal/google-places-api": "^1.7",
        "spacemudd/laravel-tap-payments": "^2.0",
        "spatie/laravel-backup": "^6.14",
        "spatie/laravel-medialibrary": "^8.9",
        "spatie/laravel-permission": "^3.17",
        "spatie/laravel-query-builder": "^4.0",
        "spatie/laravel-searchable": "^1.7",
        "teamtnt/laravel-scout-tntsearch-driver": "^11.1",
        "tightenco/ziggy": "^0.9.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.8",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3",
        "spacemudd/laravel-vue-i18n-generator": "^0.1.47"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "ext-pcntl": "7.3",
            "ext-posix": "7.3"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "treeware/plant": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Classes/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
